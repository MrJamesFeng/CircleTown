/* pages/classify/index.wxss */

page {
  background-color: #dedede;
}

.typebrand-slider {
  display: inline-block;
  width: 25%;
  background: #f5f5f5;
  height: 100%;
  text-align: center;
}

.typebrand-desc {
  height: 25px;
  line-height: 25px;
  border-bottom: 1px solid #dedede;
  padding: 6px 0;
}

.typebrand-desc-slected {
  height: 25px;
  line-height: 25px;
  border-bottom: 1px solid #dedede;
  background-color: burlywood;
}

.typebrand-brands {
  position: absolute;
  right: 0;
  top: 0;
  width: 75%;
}

.selectedNode-img {
  width: 33%;
  height: 100rpx;
}
